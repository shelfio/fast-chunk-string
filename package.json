{
  "name": "fast-chunk-string",
  "version": "1.0.1",
  "description": "Chunk string into equal substrings with unicode support",
  "license": "MIT",
  "repository": "vladgolubev/fast-chunk-string",
  "author": {
    "name": "Vlad Holubiev",
    "email": "golubev.vld@gmail.com",
    "url": "vladholubiev.com"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "benchmark": "node benchmark",
    "coverage": "jest --coverage",
    "lint": "eslint . --fix",
    "test": "jest"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "string",
    "split",
    "chunk",
    "unicode",
    "utf",
    "utf8",
    "substring"
  ],
  "dependencies": {
    "runes": "0.4.3",
    "string-length": "3.1.0"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.11.1",
    "benchmark": "2.1.4",
    "eslint": "6.7.1",
    "fast-lorem-ipsum": "1.2.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write --ignore-path=./.eslintignore",
      "git add"
    ]
  }
}
